
@{
    ViewData["Title"] = "Yes Active Account Lead";
    Layout = "~/Views/Layouts/_adminLayout.cshtml";
}


<div class="page-wrapper">


    <div class="container-fluid">
        <br />
        <br />
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 style=" font-size: 25px; font-weight: bold;" class="card-title">CONSUMER LEAD INFO'S</h4>

                        <div class="table-responsive m-t-40 m-t-400">
                            <table class="demo-foo-addrow2 table table-bordered table m-b-0 toggle-arrow-tiny example23" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th data-toggle="true">First Name</th>
                                        <th>Last Name</th>
                                        <th data-hide="all">Address</th>

                                        <th data-hide="all">Zip Code</th>
                                        <th data-hide="all">Phone</th>
                                        <th data-hide="all">E-mail</th>
                                        <th data-hide="all">Active Account</th>
                                        <th data-hide="all">
                                            NetSpend
                                        </th>
                                        <th data-hide="all">Page</th>
                                        <th data-hide="all">Device ID</th>
                                        <th data-hide="all">Device Type</th>
                                        <th data-hide="all">IP Address</th>
                                        <th data-hide="all">Date</th>
                                        <th data-hide="phone">Action</th>

                                    </tr>
                                </thead>

                                <tbody>

                                    @foreach (var i in ViewBag.users)
                                    {
                                        <tr>

                                            <td>

                                                <a>@i.First_Name</a>

                                            <td>
                                                <a href="javascript:void(0);">@i.Last_Name</a>
                                            </td>

                                            <td>@i.address</td>
                                            <td>@i.zip_code</td>
                                            <td>@i.phone</td>
                                            <td>@i.email</td>
                                            <td>@i.checkingaccount</td>
                                            <td>@i.netspend</td>
                                            <td>@i.forms</td>

                                            <td>@i.deviceid</td>
                                            <td>@i.devicetype</td>
                                            <td>@i.Ipaddress</td>
                                            <td>@i.date</td>

                                            <td>   <a onclick="return checkDelete()"> @Html.ActionLink(" ", "Deleteconsumer", "admin", null, null, "", new { tid = i.Id }, new { @class = "fa fa-close text-danger", onclick = "return checkDelete()", title = "Archive" })</a></td>






                                        </tr>

                                    }
                                </tbody>

                            </table>
                        </div>
                        <script type="text/javascript">
                            function checkDelete() {
                                if (confirm('Are you sure you want to delete ?')) {

                                    return true
                                }
                                else {
                                    return false
                                }
                            }
                        </script>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>



@if (ViewBag.response == "Delete")
{

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <script>swal("Consumer Record Deleted Successfully", "", "success");</script>


    <script>
        Swal.fire({
            title: 'هل تريد الاستمرار؟',
            icon: 'question',
            iconHtml: '؟',
            confirmButtonText: 'نعم',
            cancelButtonText: 'لا',
            showCancelButton: true,
            showCloseButton: true
        })
    </script>
}
