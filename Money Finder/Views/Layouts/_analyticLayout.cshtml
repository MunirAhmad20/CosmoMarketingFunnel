<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/nfd/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <link href="~/nfd/assets/plugins/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet" />
    <link href="~/nfd/assets/plugins/metismenu/css/metisMenu.min.css" rel="stylesheet" />
    <!-- loader-->
    <link href="~/nfd/assets/css/pace.min.css" rel="stylesheet" />
    <script src="~/nfd/assets/js/pace.min.js"></script>
    <!-- Bootstrap CSS -->
    <link href="~/nfd/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/nfd/assets/css/bootstrap-extended.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&display=swap" rel="stylesheet">
    <link href="~/nfd/assets/css/app.css" rel="stylesheet">
    <link href="~/nfd/assets/css/icons.css" rel="stylesheet">
    <!-- Theme Style CSS -->
    <link rel="stylesheet" href="~/nfd/assets/css/dark-theme.css" />
    <link rel="stylesheet" href="~/nfd/assets/css/semi-dark.css" />
    <link rel="stylesheet" href="~/nfd/assets/css/header-colors.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    
    <style>
        .sideheder {
            width: 250px;
            height: 60px;
            display: flex;
            align-items: center;
            padding: 0 15px;
            z-index: 5;
            background: #f7f7ff;
            background-clip: padding-box;
            border-bottom: 1px solid #e4e4e4;
        }
        .h3bg {
            background: linear-gradient(90deg, #1E292B 0%, #273132 20.83%, #313735 40.62%, #3E413A 60.94%, #4A4B43 80.21%, #5A5748 100%);
            color: white;
            height: 40px;
            font-size: 16px;
            font-weight: 600;
            padding-left: 2%;
            padding-top: 11px;
            /* border-radius: 10px; */
            border-top-left-radius: 7px;
            border-top-right-radius: 7px;
        }
    </style>
</head>
<body>
    <div class="wrapper">
        <div class="sidebar-wrapper" data-simplebar="true">
            <div class="sidebar-header">

                <div>
                    <h4 style=" color: black; font-weight: 700;" class="logo-text">Date Range</h4>
                </div>
                <div>
                    <i class="lni lni-question-circle" style="margin-left: 5px;"></i>
                </div>
            </div>
           
            <!--navigation-->

            <ul class="metismenu" id="menu">
                <select class="form-control">
                    <option>Yesterday</option>
                    <option>Yesterday</option>
                    <option>Yesterday</option>


                </select>
                <div class="sideheder" style=" margin-top: 1em;">

                    <div>
                        <h4 style=" color: black; font-weight: 700;" class="logo-text">Attributes</h4>
                    </div>

                </div>
                <a href="javascript:;" class="has-arrow">


                    <div style="color: black; font-weight: 600;" class="menu-title">OverView</div>
                </a>
                <a href="javascript:;" class="has-arrow">


                    <div style="color: black; font-weight: 600;" class="menu-title">Browser</div>
                </a>
                <a href="javascript:;" class="has-arrow">


                    <div style="color: black; font-weight: 600;" class="menu-title">OS</div>
                </a>



                <div class="sideheder" style=" margin-top: 1em;">

                    <div>
                        <h4 style=" font-size: 17px;color: black; font-weight: 700;margin-left: -10px;" class="logo-text">Filter by Compaign Type </h4>
                    </div>

                </div>
                <div class="form-check" style="font-weight: 600;
    margin-top: 1em;">
                    <input class="form-check-input" type="checkbox" value="">
                    <label class="form-check-label">Call Center</label>
                </div>
                <div class="form-check" style="font-weight: 600;">
                    <input class="form-check-input" type="checkbox" value="">
                    <label class="form-check-label">Web</label>
                </div>

                <div class="sideheder" style=" margin-top: 1em;">

                    <div>
                        <h4 style=" font-size: 17px;color: black; font-weight: 700;margin-left: -10px;" class="logo-text">Filter by Compaign  </h4>
                    </div>

                </div>
                <h5 style="height: 1.6em;margin-top: 6px;border-bottom: 1px solid #e4e4e4;">Select: All | None</h5>
                <div class="form-check" style="font-weight: 600;
    margin-top: 1em;">
                    <input class="form-check-input" type="checkbox" value="">
                    <label class="form-check-label">ID 72: Auto Site Short Form</label>
                </div>
               

            </ul>
        </div>
    
        <div class="page-wrapper">
            <div class="page-content">
               
                    @RenderBody()
               
                </div>
                </div>
                </div>
</body>
</html>
