<style>
    .hunderd {
        font-size: 12px;
        padding: 5px;
        border-radius: 5px;
        color: #fff;
        /* margin-left: 5px; */
        /* text-align: right; */
        background-color: #e53935;
        max-width: 46px;
        height: 28px;
        float: right;
        width: 32px;
    }

    .uppertext {
        text-transform: uppercase;
        font-size: 13.5px;
        color: #555;
        margin-right: 5px;
    }

    .box {
        box-shadow: 0 1px 5px 0 rgb(0 0 0 / 20%), 0 2px 2px 0 rgb(0 0 0 / 14%), 0 3px 1px -2px rgb(0 0 0 / 12%);
    }

    .pacingname {
        letter-spacing: .2px;
        color: rgba(0, 0, 0, .87);
        text-align: left;
        text-transform: uppercase;
        font-size: 10px;
        color: #555;
        margin-bottom: 1rem !important;
    }

    .zeropacingname {
        float: right;
        letter-spacing: .2px;
        color: rgba(0, 0, 0, .87);
        text-align: left;
        text-transform: uppercase;
        font-size: 10px;
        color: #555;
        margin-bottom: 1rem !important;
    }

    .metric-today {
        text-transform: uppercase;
        font-size: 10px;
        margin-top: 2px;
        color: #555;
    }

    .main-metric {
        color: rgba(0, 0, 0, .87);
        text-align: center;
        margin-top: 1em;
        margin-bottom: 1em;
    }
</style>




