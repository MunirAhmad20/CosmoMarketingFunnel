@foreach (var i in ViewBag.sites)
{
<script>

        function redirect() {
           
            var web = $("#change").val();
            if (web == "Senior Discount Savings") {
                window.location.href = "http://SeniorDiscountSavings.com/admin/valuepass?tid=@i.Gmail&tidpass=@i.password"

            }
            if (web == "AutoRate Freedom") {
                window.location.href = "https://autoratefreedom.com/admin/valuepass?tid=@i.Gmail&tidpass=@i.password"

            }
            if (web == "Unclaimed GOV") {
                window.location.href = "http://unclaimedgovernment.com/admin/valuepass?tid=@i.Gmail&tidpass=@i.password"

            }
            if (web == "Student Fintech") {
                window.location.href = "http://studentfintech.app/admin/valuepass?tid=@i.Gmail&tidpass=@i.password"


            }
            if (web == "CCG Temp") {
                window.location.href = "http://cosmopolitantemplate.com/admin/valuepass?tid=@i.Gmail&tidpass=@i.password"

            }
            if (web == "CCG") {
                window.location.href = "http://cosmopolitancg.com"

            }


        }




</script>
