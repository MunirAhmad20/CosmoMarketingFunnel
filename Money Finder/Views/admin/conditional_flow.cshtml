<form id="formonoff">
    @foreach (var it in ViewBag.qa)
    {

        if (it.Validation == "on")
        {


            <label class="switch" style="float: right; margin-top: 0.2%; ">
                <input onclick="onoff();" type="checkbox" checked>
                <span class="slider round"></span>
            </label>
        }
        if (it.Validation == "off")
        {


            <label class="switch" style="float: right; margin-top: 0.2%; ">
                <input onclick="onoff();" type="checkbox" >
                <span class="slider round"></span>
            </label>
        }
    }
</form>
